{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "esnext",
        "target": "es2020",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "noImplicitThis": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "allowJs": false,
    }
}